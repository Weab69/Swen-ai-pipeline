{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/swen/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/swen/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/swen",
  "projects": {
    "ai": {
      "type": "application",
      "root": "apps/ai",
      "entryFile": "main",
      "sourceRoot": "apps/ai/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ai/tsconfig.app.json"
      }
    },
    "ingestion": {
      "type": "application",
      "root": "apps/ingestion",
      "entryFile": "main",
      "sourceRoot": "apps/ingestion/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ingestion/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "storage": {
      "type": "application",
      "root": "apps/storage",
      "entryFile": "main",
      "sourceRoot": "apps/storage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/storage/tsconfig.app.json"
      }
    },
    "swen": {
      "type": "application",
      "root": "apps/swen",
      "entryFile": "main",
      "sourceRoot": "apps/swen/src",
      "compilerOptions": {
        "tsConfigPath": "apps/swen/tsconfig.app.json"
      }
    },
    "news": {
      "type": "application",
      "root": "apps/news",
      "entryFile": "main",
      "sourceRoot": "apps/news/src",
      "compilerOptions": {
        "tsConfigPath": "apps/news/tsconfig.app.json"
      }
    }
  }
}
